<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#8c9694" />
            <gradient android:angle="-90" android:startColor="#25B84E" android:endColor="#25B84E"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#8c9694" />
            <solid android:color="#25B84E"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#25B84E" />
            <gradient android:angle="-90" android:startColor="#25B84E" android:endColor="#25B84E" />
        </shape>
    </item>
</selector>